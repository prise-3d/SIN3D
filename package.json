{
  "name": "expe-web",
  "version": "0.0.8",
  "private": true,
  "scripts": {
    "server:start": "node -r esm server/index.js",
    "server:dev": "nodemon --exec npm run server:start",
    "server:lint": "eslint server/ --fix",
    "app:dev": "vue-cli-service serve",
    "app:build": "vue-cli-service build",
    "app:lint": "vue-cli-service lint",
    "test": "node test/api/_test_setup_start.js && ava --verbose && node test/api/_test_setup_stop.js"
  },
  "dependencies": {
    "boom": "^7.3.0",
    "compression": "^1.7.4",
    "core-js": "^2.6.5",
    "cors": "^2.8.5",
    "esm": "^3.2.22",
    "express": "^4.16.4",
    "helmet": "^3.16.0",
    "morgan": "^1.9.1",
    "serve-static": "^1.13.2",
    "sharp": "^0.22.0",
    "vue": "^2.6.6",
    "vue-router": "^3.0.1",
    "winston": "^3.2.1",
    "ws": "^6.2.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.5.0",
    "@vue/cli-plugin-eslint": "^3.5.0",
    "@vue/cli-service": "^3.5.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "ava": "^1.4.1",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "fs-extra": "^7.0.1",
    "nodemon": "^1.18.10",
    "supertest": "^4.0.2",
    "vue-template-compiler": "^2.5.21"
  },
  "ava": {
    "require": [
      "esm"
    ]
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/recommended",
      "@vue/standard"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
